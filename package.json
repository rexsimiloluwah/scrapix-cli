{
  "name": "scrapix-cli",
  "version": "0.1.5",
  "description": "A TypeScript-based CLI Application for scraping Google images",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --pretty src/index.ts",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "scrapix": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rexsimiloluwah/scrapix-cli.git"
  },
  "keywords": [
    "image-scraping",
    "typescript",
    "cli",
    "headless"
  ],
  "author": "theblackdove",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rexsimiloluwah/scrapix-cli/issues"
  },
  "homepage": "https://github.com/rexsimiloluwah/scrapix-cli#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^8.1.0",
    "figlet": "^1.5.0",
    "images-scraper": "^6.3.0",
    "inquirer": "^8.1.2",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^7.3.3",
    "@types/request": "^2.48.7",
    "standard-version": "^9.3.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
